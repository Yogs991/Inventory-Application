<html>
    <body>
        <%- include('../partials/navbar.ejs') %>
        <title><%= setName %></title>
        <div class="cardList-container">
        <% if(cards && cards.length>0){ %>
            <div class="cardList">
                <% cards.forEach(card=>{ %>
                    <div class="card-info">
                        <img src="<%= card.image %>/low.png" alt="<%= card.name %>">
                        <p><%= card.name %></p>
                        <div class="view-details">
                            <a href="/cardDetails/<%= card.id %>">View Details</a>
                        </div>
                    </div>
                <% }); %>
            </div>
        <% }%>
        </div>
    </body>
</html>