<html>
    <head>
        <link rel="stylesheet" href="/css/style.css">
    </head>
    <body>
        <%- include('../partials/navbar.ejs') %>
        <h1>My Collection</h1>
        <% cardInfo.forEach(card=>{ %>
            <div class="card-info"> <!-- change class name-->
                <img src="<%= card.picture%>" alt="card image">
                <h2><%= card.name %></h2>
                <h2><%= card.expansion %></h2>
                <p><%= card.price %></p>
            </div>
            <img src="/delete_icon.png" alt="delete icon">
        <% }); %>
    </body>
</html>